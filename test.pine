extern puts :: fn(s: cstring) i32;
extern putchar :: fn(c: i32) i32;

main :: fn(args: []string) void {
    for (args) [arg] {
        puts(arg.ptr);
    }

    nums := [2]i32{6, 7};
    for (nums) [n] {
        putchar(cast(i32)n + cast(i32)'0');
    }
    putchar(cast(i32)'\n');

    for (0..10) [i] {
        putchar(cast(i32)i + cast(i32)'0');
    }
    putchar(cast(i32)'\n');
}
